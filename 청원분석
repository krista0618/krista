      "source": [
        "meanvotes_1 = Preform.groupby(['startyear','startmonth']).mean()\n",
        "meanvotes_1 = meanvotes_1.rename(columns = {'votes':'votes1'})\n",
        "\n",
        "meanvotes_2 = Hrights.groupby(['startyear','startmonth']).mean()\n",
        "meanvotes_2 = meanvotes_2.rename(columns = {'votes':'votes2'})\n",
        "\n",
        "meanvotes_3 = Safety.groupby(['startyear','startmonth']).mean()\n",
        "meanvotes_3 = meanvotes_3.rename(columns = {'votes':'votes3'})\n",
        "\n",
        "meanvotes_4 = Development.groupby(['startyear','startmonth']).mean()\n",
        "meanvotes_4 = meanvotes_4.rename(columns = {'votes':'votes4'})\n",
        "\n",
        "meanvotes_5 = Diplomacy.groupby(['startyear','startmonth']).mean()\n",
        "meanvotes_5 = meanvotes_5.rename(columns = {'votes':'votes5'})\n",
        "\n",
        "col_concat = pd.concat([meanvotes_1,meanvotes_2,meanvotes_3,meanvotes_4,meanvotes_5],axis = 1)\n",
        "\n",
        "col_concat = col_concat.drop(['article_id', 'endyear','endmonth','answered'],axis = 1)\n",
        "col_concat\n",
        "\n",
        "\n",
