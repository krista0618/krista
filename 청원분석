      "source": [
        "def my_percent(x):\n",
        "  return x/Development['votes'].sum()* 100\n",
        "Dev = Development.sort_values('votes',ascending = False).head(5)\n",
        "Dev_rest = Development.sort_values('votes',ascending = False)[6:]\n",
        "\n",
        "\n",
        "df = pd.DataFrame({'votes': Dev['votes']})\n",
        "df2 = pd.DataFrame({'votes': Dev_rest['votes']})\n",
        "df['percent'] = df['votes'].apply(my_percent)\n",
        "df2['percent'] = df2['votes'].apply(my_percent).sum()\n",
        "\n",
        "rest = df2.iloc[0:1]\n",
        "rest.index = ['rest']\n",
        "\n",
        "df.index = [1,2,3,4,5]\n",
        "\n",
        "pie = pd.concat([df,rest])\n",
        "pie['percent']. plot(kind = 'pie')\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n",
        "\n"
