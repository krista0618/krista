        "\n",
        "Preform = (politics['category'] == '정치개혁')\n",
        "Hrights = (politics['category'] =='인권/성평등')\n",
        "Safety = (politics['category'] == '안전/환경')\n",
        "Development = (politics['category'] =='교통/건축/국토')\n",
        "Diplomacy = (politics['category'] == '외교/통일/국방')\n",
        "\n",
        "Preform = politics[Preform]\n",
        "Hrights = politics[Hrights]\n",
        "Safety = politics[Safety]\n",
        "Development = politics[Development]\n",
        "Diplomacy = politics[Diplomacy]\n",
        "\n"
